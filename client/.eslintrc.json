{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "react/prop-types": "off",
    "no-console": "warn",
    "quotes": [2, "single"],
    "jsx-quotes": ["warn", "prefer-single"],
    "prefer-const": "warn",
    "indent": ["warn", 4],
    "comma-dangle": ["error", "never"],
    "semi": ["error", "never"],
    "import/order": ["warn", {
      "groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"]
    }]
  }
}